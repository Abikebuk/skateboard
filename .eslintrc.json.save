{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["warn", "never"],
        "require-jsdoc": "off",
        "react/react-in-jsx-scope": "off",
        "no-unused-vars": "warn",
        "max-len": ["warn", {"code" :  100}],
        "react/no-unknown-property": ["off", { "ignore": [] }]
    }
}
